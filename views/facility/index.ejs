<h1>Facilities</h1>
<div id="facility-list">
    <% facilities.forEach(facility => { %>
        <div class="facility-card">
            <h2><%= facility.name %></h2>
            <p><%= facility.description %></p>
            <button onclick="window.location.href='/facilities/detail?id=<%= facility._id %>'">View Details</button>
        </div>
    <% }); %>
</div>